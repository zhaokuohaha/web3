@model IEnumerable<web3.Domain.Entities.Web_User>

@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_HomeLayout.cshtml";
}

<h2>UserList</h2>
<table class="table table-bordered">
    <tr>
        <th>@Html.DisplayNameFor(model => model.u_name)</th>
        <th>@Html.DisplayNameFor(model => model.u_email)</th>
        <th>@Html.DisplayNameFor(model => model.u_image)</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.u_name</td>
            <td>@item.u_email</td>
            <td>@item.u_image</td>
        </tr>
    }
</table>